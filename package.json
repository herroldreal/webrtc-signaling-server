{
  "name": "webrtc-backend",
  "version": "0.0.1",
  "description": "WebRTC Signaling Server",
  "author": "Herrold H. Real",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@aws-sdk/client-apigatewaymanagementapi": "3.301.0",
    "@aws-sdk/client-lambda": "3.301.0",
    "@nestjs/common": "^9.3.10",
    "@nestjs/config": "2.3.1",
    "@nestjs/core": "^9.3.10",
    "@nestjs/jwt": "10.0.2",
    "@nestjs/platform-express": "^9.3.10",
    "@nestjs/platform-socket.io": "9.3.10",
    "@nestjs/websockets": "9.3.10",
    "@socket.io/redis-adapter": "8.1.0",
    "ajv": "8.12.0",
    "aws-api-gateway-client": "0.3.7",
    "aws-serverless-express": "3.4.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "ioredis": "5.3.1",
    "json-schema-traverse": "1.0.0",
    "nanoid": "3.3.4",
    "redis": "4.6.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "socket.io": "4.6.1"
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "3.301.0",
    "@aws-sdk/lib-dynamodb": "3.301.0",
    "@nestjs/cli": "9.3.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.10",
    "@types/aws-lambda": "8.10.114",
    "@types/aws-sdk": "2.7.0",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "aws-lambda": "1.0.7",
    "aws-sdk": "2.1345.0",
    "aws-sdk-client-mock": "2.1.1",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.5.0",
    "plugin": "0.3.3",
    "prettier": "^2.8.5",
    "serverless-dynamodb-local": "0.2.40",
    "serverless-offline": "12.0.4",
    "serverless-plugin-optimize": "4.2.1-rc.1",
    "serverless-plugin-warmup": "8.2.1",
    "serverless-webpack": "5.11.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.2",
    "tsconfig-paths-webpack-plugin": "4.0.0",
    "typescript": "^5.0.2",
    "webpack": "5.75.0",
    "webpack-node-externals": "3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": "16.14.2"
  }
}
